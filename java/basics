        Scanner s=new Scanner(System.in);
        int n1=s.nextInt();
        int n2=s.nextInt();
        int div;
        div=n1/n2;
        System.out.println(div);
    }
}



##### modulus


import java.util.Scanner;
class Sample{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n1=s.nextInt();
        int n2=s.nextInt();
        int mod;
        mod=n1%n2;
        System.out.println(mod);
    }
}



